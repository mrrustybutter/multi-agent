{
  "name": "@rusty-butter/shared",
  "version": "0.1.0",
  "description": "Shared utilities for multi-agent system",
  "type": "module",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./mcp-connection": {
      "import": "./dist/mcp-connection.js",
      "types": "./dist/mcp-connection.d.ts"
    },
    "./queue-manager": {
      "import": "./dist/queue-manager.js",
      "types": "./dist/queue-manager.d.ts"
    },
    "./memory-integration": {
      "import": "./dist/memory-integration.js",
      "types": "./dist/memory-integration.d.ts"
    },
    "./health-monitor": {
      "import": "./dist/health-monitor.js",
      "types": "./dist/health-monitor.d.ts"
    },
    "./config-client": {
      "import": "./dist/config-client.js",
      "types": "./dist/config-client.d.ts"
    }
  },
  "scripts": {
    "build": "tsc",
    "dev": "tsc --watch",
    "typecheck": "tsc --noEmit"
  },
  "dependencies": {
    "@elevenlabs/elevenlabs-js": "^2.8.0",
    "@google/generative-ai": "^0.2.1",
    "@modelcontextprotocol/sdk": "^1.17.0",
    "@rusty-butter/logger": "workspace:*",
    "chokidar": "^3.5.3",
    "mongoose": "^8.17.0",
    "openai": "^4.28.4",
    "uuid": "^9.0.1",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/node": "^24.2.0",
    "@types/uuid": "^9.0.7",
    "typescript": "^5.9.2"
  }
}